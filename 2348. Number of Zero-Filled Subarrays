class Solution {
public:
    long long zeroFilledSubarray(vector<int>& nums) {
        int i = 0, j = 0, n = nums.size(), ans = 0;
            for( i = 0; i< n; i++ )
            {
                    j = i;
                    if(nums[i] == 0)
                    {
                            ans++;
                            j = i + 1;
                            while(j < n && nums[j] == 0)
                            {
                                    ans++;
                                    j++;
                            }
                            
                    }
                    
                    
            }
            return ans;
    }
};
